<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.manage.mapper.BoardCustRegMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="boardCustRegResultMap" type="com.minigod.zero.manage.entity.BoardCustRegEntity">
        <result column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="board_id" property="boardId"/>
        <result column="open_count" property="openCount"/>
        <result column="status" property="status"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="is_delete" property="isDelete"/>
    </resultMap>


    <select id="selectBoardCustRegPage" resultMap="boardCustRegResultMap">
        select * from oms_board_cust_reg where is_deleted = 0
    </select>


</mapper>
