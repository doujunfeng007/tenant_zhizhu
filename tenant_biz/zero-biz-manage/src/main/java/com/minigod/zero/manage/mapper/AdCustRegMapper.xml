<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.manage.mapper.AdCustRegMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="adCustRegResultMap" type="com.minigod.zero.manage.entity.AdCustRegEntity">
        <result column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="ad_id" property="adId"/>
        <result column="open_count" property="openCount"/>
        <result column="is_close" property="isClose"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="status" property="status"/>
        <result column="is_deleted" property="isDeleted"/>
    </resultMap>


    <select id="selectAdCustRegPage" resultMap="adCustRegResultMap">
        select * from oms_ad_cust_reg where is_deleted = 0
    </select>


</mapper>
