<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.bpmn.module.deposit.mapper.SecBankIdConfigMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="baseResultMap" type="com.minigod.zero.bpmn.module.deposit.entity.SecBankIdConfigEntity">
        <result column="id" property="id"/>
        <result column="bank_type" property="bankType"/>
        <result column="bank_name" property="bankName"/>
        <result column="bank_code" property="bankCode"/>
        <result column="bank_id" property="bankId"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="tenant_id" property="tenantId"/>
    </resultMap>


    <select id="selectSecBankIdConfigPage" resultMap="baseResultMap">
        select * from sec_bank_id_config where is_deleted = 0
    </select>


</mapper>
