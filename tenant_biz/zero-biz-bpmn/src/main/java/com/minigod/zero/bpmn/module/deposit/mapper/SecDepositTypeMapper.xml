<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.bpmn.module.deposit.mapper.SecDepositTypeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="baseResultMap" type="com.minigod.zero.bpmn.module.deposit.entity.SecDepositTypeEntity">
        <result column="id" property="id"/>
        <result column="deposit_type" property="depositType"/>
        <result column="charge_money" property="chargeMoney"/>
        <result column="charge_money_remark" property="chargeMoneyRemark"/>
        <result column="time_arrival" property="timeArrival"/>
        <result column="time_arrival_remark" property="timeArrivalRemark"/>
        <result column="bank_name" property="bankName"/>
        <result column="bank_code" property="bankCode"/>
        <result column="receipt_account_name" property="receiptAccountName"/>
        <result column="receipt_account_name_remark" property="receiptAccountNameRemark"/>
        <result column="pc_guide_url" property="pcGuideUrl"/>
        <result column="app_guide_url" property="appGuideUrl"/>
        <result column="deposit_cert_img" property="depositCertImg"/>
        <result column="founder" property="founder"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="account_fps" property="accountFps"/>
        <result column="tenant_id" property="tenantId"/>
    </resultMap>


    <select id="selectSecDepositTypePage" resultMap="baseResultMap">
        select * from sec_deposit_type where is_deleted = 0
    </select>


</mapper>
