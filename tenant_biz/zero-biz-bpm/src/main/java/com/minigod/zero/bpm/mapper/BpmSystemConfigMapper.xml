<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.bpm.mapper.BpmSystemConfigMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="bpmSystemConfigResultMap" type="com.minigod.zero.bpm.entity.BpmSystemConfigEntity">
        <result column="id" property="id"/>
        <result column="key" property="key"/>
        <result column="value" property="value"/>
        <result column="status" property="status"/>
        <result column="remark" property="remark"/>
        <result column="key_id" property="keyId"/>
        <result column="create_time" property="createTime"/>
        <result column="create_user" property="createUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="update_user" property="updateUser"/>
    </resultMap>


    <select id="selectBpmSystemConfigPage" resultMap="bpmSystemConfigResultMap">
        select * from bpm_system_config where is_deleted = 0
    </select>


</mapper>
