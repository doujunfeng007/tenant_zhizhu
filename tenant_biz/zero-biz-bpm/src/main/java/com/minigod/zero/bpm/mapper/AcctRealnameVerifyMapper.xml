<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.bpm.mapper.AcctRealnameVerifyMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="acctRealnameVerifyResultMap" type="com.minigod.zero.bpm.entity.AcctRealnameVerifyEntity">
        <result column="id" property="id"/>
        <result column="cust_id" property="custId"/>
        <result column="real_name" property="realName"/>
        <result column="idcard" property="idcard"/>
        <result column="sequence" property="sequence"/>
        <result column="verifie_err_count" property="verifieErrCount"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="lock_version" property="lockVersion"/>
    </resultMap>


    <select id="selectAcctRealnameVerifyPage" resultMap="acctRealnameVerifyResultMap">
        select * from acct_realname_verify where is_deleted = 0
    </select>


</mapper>
