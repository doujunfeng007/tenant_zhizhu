<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.bpm.mapper.AcctOpenImageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="acctOpenImageResultMap" type="com.minigod.zero.bpm.entity.AcctOpenImageEntity">
        <result column="id" property="id"/>
        <result column="cust_id" property="custId"/>
        <result column="create_time" property="createTime"/>
        <result column="image_location" property="imageLocation"/>
        <result column="image_location_type" property="imageLocationType"/>
        <result column="img_path" property="imgPath"/>
        <result column="error_status" property="errorStatus"/>
        <result column="source" property="source"/>
    </resultMap>


    <select id="selectAcctOpenImagePage" resultMap="acctOpenImageResultMap">
        select * from acct_open_image where is_deleted = 0
    </select>


</mapper>
