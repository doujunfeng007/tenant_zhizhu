<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.bpm.mapper.AcctOpenInfoTempMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="acctOpenInfoTempResultMap" type="com.minigod.zero.bpm.entity.AcctOpenInfoTempEntity">
        <result column="id" property="id"/>
        <result column="cust_id" property="custId"/>
        <result column="json_info" property="jsonInfo"/>
        <result column="step" property="step"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>


    <select id="selectAcctOpenInfoTempPage" resultMap="acctOpenInfoTempResultMap">
        select * from acct_open_info_temp where is_deleted = 0
    </select>


</mapper>
