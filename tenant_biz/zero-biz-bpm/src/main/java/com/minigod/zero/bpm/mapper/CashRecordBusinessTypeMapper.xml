<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.bpm.mapper.CashRecordBusinessTypeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="cashRecordBusinessTypeResultMap" type="com.minigod.zero.bpm.entity.CashRecordBusinessTypeEntity">
        <result column="id" property="id"/>
        <result column="business_type" property="businessType"/>
        <result column="business_key" property="businessKey"/>
        <result column="remark" property="remark"/>
        <result column="key_value" property="keyValue"/>
        <result column="status" property="status"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>


    <select id="selectCashRecordBusinessTypePage" resultMap="cashRecordBusinessTypeResultMap">
        select * from cash_record_business_type where is_deleted = 0
    </select>


</mapper>
