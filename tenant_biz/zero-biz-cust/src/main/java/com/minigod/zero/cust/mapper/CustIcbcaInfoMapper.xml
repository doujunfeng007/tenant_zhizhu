<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.cust.mapper.CustIcbcaInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="icbcaInfoResultMap" type="com.minigod.zero.cust.entity.CustIcbcaInfoEntity">
        <result column="id" property="id"/>
        <result column="cust_id" property="custId"/>
        <result column="ci_no" property="ciNo"/>
        <result column="medium_no" property="mediumNo"/>
        <result column="medium_id" property="mediumId"/>
        <result column="create_time" property="createTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="is_deleted" property="isDeleted"/>
    </resultMap>


    <select id="selectCustIcbcaInfoPage" resultMap="icbcaInfoResultMap">
        select * from cust_icbca_info where is_deleted = 0
    </select>


</mapper>
