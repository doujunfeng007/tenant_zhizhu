<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.customer.mapper.CustomerResetLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="resetLogResultMap" type="com.minigod.zero.customer.entity.CustomerResetLogEntity">
        <result column="id" property="id"/>
        <result column="cust_id" property="custId"/>
        <result column="tenant_id" property="tenantId"/>
        <result column="rest_time" property="restTime"/>
        <result column="rest_type" property="restType"/>
        <result column="create_user" property="createUser"/>
        <result column="create_dept" property="createDept"/>
        <result column="create_time" property="createTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="status" property="status"/>
        <result column="is_deleted" property="isDeleted"/>
    </resultMap>


    <select id="selectCustomerResetLogPage" resultMap="resetLogResultMap">
        select * from customer_reset_log where is_deleted = 0
    </select>


</mapper>
