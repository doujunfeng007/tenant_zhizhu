<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.trade.mapper.IpoFinanceRateMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="ipoFinanceRateResultMap" type="com.minigod.zero.trade.entity.IpoFinanceRateEntity">
        <result column="id" property="id"/>
        <result column="asset_id" property="assetId"/>
        <result column="finance_rate" property="financeRate"/>
        <result column="ctime" property="ctime"/>
        <result column="mtime" property="mtime"/>
    </resultMap>


    <select id="selectIpoFinanceRatePage" resultMap="ipoFinanceRateResultMap">
        select * from ipo_finance_rate where is_deleted = 0
    </select>


</mapper>
