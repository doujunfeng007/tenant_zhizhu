<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minigod.zero.trade.mapper.IpoGeniusInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="ipoGeniusInfoResultMap" type="com.minigod.zero.trade.entity.IpoGeniusInfoEntity">
        <result column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="trade_pwd" property="tradePwd"/>
        <result column="op_station" property="opStation"/>
        <result column="initial_amount" property="initialAmount"/>
        <result column="subscribe_number" property="subscribeNumber"/>
        <result column="is_status" property="isStatus"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>


    <select id="selectIpoGeniusInfoPage" resultMap="ipoGeniusInfoResultMap">
        select * from ipo_genius_info where is_deleted = 0
    </select>


</mapper>
